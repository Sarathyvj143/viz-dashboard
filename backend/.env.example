# Application
APP_NAME=VisualizationPlatform
APP_VERSION=1.0.0
DEBUG=false

# Security (REQUIRED) - GENERATE NEW VALUES, DO NOT USE EXAMPLES BELOW
# Generate SECRET_KEY: python -c 'import secrets; print(secrets.token_urlsafe(32))'
SECRET_KEY=REPLACE_ME_RUN_COMMAND_ABOVE_TO_GENERATE
# Generate ENCRYPTION_MASTER_KEY: python -c 'import secrets; print(secrets.token_urlsafe(32))'
ENCRYPTION_MASTER_KEY=REPLACE_ME_RUN_COMMAND_ABOVE_TO_GENERATE
# Generate ENCRYPTION_SALT: python -c 'import secrets; print(secrets.token_urlsafe(16))'
ENCRYPTION_SALT=REPLACE_ME_RUN_COMMAND_ABOVE_TO_GENERATE

# Database
SQLITE_PATH=app_metadata.db

# Redis (optional)
REDIS_URL=redis://redis:6379/0
REDIS_ENABLED=false

# CORS
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Export Settings
EXPORT_PATH=
MAX_EXPORT_SIZE_MB=50

# Query Limits
MAX_QUERY_RESULTS=10000
QUERY_TIMEOUT_SECONDS=30

# Admin user setup (optional - random password generated if not set)
# ADMIN_DEFAULT_PASSWORD=
# ADMIN_EMAIL=admin@example.com
